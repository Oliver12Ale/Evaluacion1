<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <link type="text/css" rel="stylesheet" href="~/lib/twitter-bootstrap/css/bootstrap.css" />
    <link type="text/css" rel="stylesheet" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" />
    <link type="text/css" rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/Site.css?v=@DateTime.Now.ToString("ddMMyyyyhhmmss")" />
    <link type="text/css" rel="stylesheet" href="~/lib/select2/css/select2.min.css"/>
    <link type="text/css" rel="stylesheet" href="~/lib/select2-bootstrap-theme/select2-bootstrap.min.css" />



</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light" style="">
        <a class="navbar-brand" href="#">Evaluacion</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavDropdown">
            <ul class="navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:60572/Preguntas/Index">Preguntas</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:60572/Tecnologias/Index">Tecnologias</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:60572/Niveles/Index">Niveles</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="http://localhost:60572/Usuarios/Index">Usuarios</a>
                </li>
            </ul>
        </div>
    </nav>



    <div class="container body-content">
        @Html.Partial("~/Views/Shared/_Toast.cshtml")
        @Html.Partial("~/Views/Shared/_del.cshtml")

        @RenderBody()
        
        <footer class="page-footer font-small blue footer ">

            

        </footer>
    </div>

    @Html.Partial("~/Views/Shared/_Toast.cshtml")
    @Html.Partial("~/Views/Shared/_del.cshtml")
    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/twitter-bootstrap/js/bootstrap.min.js"></script>
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/lib/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/lib/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/lib/moment.js/moment.min.js"></script>
    <script src="~/Scripts/SaveAsync.js"></script>
    <script src="~/Scripts/Site.js"></script>
    <script src="~/Scripts/Toast.js"></script>
    <script src="~/Scripts/DeleteAsync.js"></script>
    <script src="~/lib/select2/js/select2.min.js"></script>
    @RenderSection("scripts", required: false)

    <script>
        $.fn.select2.defaults.set("theme", "bootstrap");

    </script>
</body>
</html>
